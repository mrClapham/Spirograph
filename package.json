{
  "name": "spirograph",
  "version": "0.0.0-semantically-released",
  "description": "A javaScript spirograph",
  "main": "src/Spirograph.js",
  "scripts": {
    "build": "rm -R dist && mkdir dist && browserify src/Spirograph.js -o dist/index.js && npm run build-demo",
    "build-demo": "webpack",
    "commit": "git-cz",
    "test": "./node_modules/.bin/karma start --single-run --browsers PhantomJS",
    "testChrome": "./node_modules/.bin/karma start --single-run --browsers Chrome",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "postinstall": "npm run build-demo",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrClapham/Spirograph"
  },
  "author": "Graham Clapham",
  "license": "MIT",
  "engines": {
    "node": "5.0.0"
  },
  "dependencies": {
    "babel-cli": "6.4.0",
    "babel-loader": "6.2.1",
    "browserify": "13.0.0",
    "webpack": "1.12.11"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "commitizen": "2.5.0",
    "cz-conventional-changelog": "1.1.5",
    "jasmine": "^2.3.2",
    "jsdocs": "^0.1.0",
    "jsdom": "^7.2.2",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-ie-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "0.2.3",
    "karma-safari-launcher": "^0.1.1",
    "phantomjs": "1.9.19",
    "semantic-release": "^4.3.5"
  }
}
